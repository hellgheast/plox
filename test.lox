class Breakfast {
    init(meat, bread) {
        this.meat = meat;
        this.bread = bread;
    }
    
    cook() {
        print "Eggs a-fryin'!";
    }

    serve(who) {
        print "Enjoy your " + this.meat + " and " + this.bread + ", " + who + ".";
    }
}

class Lunch < Breakfast {

    init(meat,bread,drink) {
        super.init(meat,bread);
        this.drink = drink;
    }

    drink() {
        print "Let's drink a Coke !";
    }
}
